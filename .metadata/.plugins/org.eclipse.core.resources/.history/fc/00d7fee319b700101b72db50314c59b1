<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd">

    <!-- Bean Declaration -->
	<bean id="student" class="com.example.model.Student">	
	</bean>
	
	<!-- First Bean -->
    <bean id="employee" class="com.example.model.Empolyee">
        <property name="empId" value="101"/>
        <property name="name" value="Shreya"/>
        <property name="city" value="Nagpur"/>
    </bean>
    
	<!-- Second Bean -->
	<bean id="emp" class="com.example.model.Empolyee">
        <property name="empId" value="102"/>
        <property name="name" value="Neha"/>
        <property name="city" value="Pune"/>
    </bean>
	
</beans>