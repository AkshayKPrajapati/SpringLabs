package com.example.model;

public class Empolyee {
	
	int empId;
	String name;
	String city;
	public int getEmpId() {
		return empId;
	}
	public void setEmpId(int empId) {
		this.empId = empId;
	}
	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getCity() {
		return city;
	}
	public void setCity(String city) {
		this.city = city;
	}
	public Empolyee() {
		super();
		System.out.println("Employee is Runing ");
		
	}
	public Empolyee(int empId, String name, String city) {
		super();
		this.empId = empId;
		this.name = name;
		this.city = city;
	}
	@Override
	public String toString() {
		return "Empolyee [empId=" + empId + ", name=" + name + ", city=" + city + "]";
	}
	
	

}
